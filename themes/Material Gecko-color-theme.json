{
  "name": "Material Gecko",
  "type": "dark",
  "colors": {
    //"contrastActiveBorder": "#122d42",
    "contrastBorder": "#0f0f0f",
    "activityBarBadge.background": "#009688",
    "activityBar.background": "#191919",
    "activityBar.foreground": "#DDDDDD",
    // This is the main background color
    "editor.background": "#212121",
    //Button
    "button.background": "#009688",
    // StatusBar
    "statusBar.background": "#191919",
    "statusBar.foreground": "#DDDDDD",
    // Widget
    "editorWidget.background": "#191919",
    "editorWidget.border": "#009688",
    // input
    "input.border": "#009688",

    // this is the main text colour
    "editor.foreground": "#d5f2ff",
    "editor.selectionBackground": "#67769660",
    "editor.selectionHighlightBackground": "#ffffff10",
    //"editor.selectionHighlightBorder": "#ddd",
    // "editor.lineHighlightBackground": "#282a2e",
    "editor.lineHighlightBackground": "#1b1b1b",
    "editorIndentGuide.background": "#3b3b3b",
    "editorLineNumber.foreground": "#424242",
    "editorBracketMatch.border": "#FFCC0050",
    "editorBracketMatch.background": "#212121",
    "editorLineNumber.activeForeground": "#d5f2ff",
    "editorCursor.foreground": "#009688",
    // sidebar
    "sideBar.background": "#191919",
    "sideBar.border": "#111111",
    "sideBar.foreground": "#DDDDDD",
    "sideBarSectionHeader.background": "#212121",
    "sideBarSectionHeader.foreground": "#d5f2ff",
    // List
    "list.inactiveSelectionForeground": "#d5f2ff",
    "list.activeSelectionForeground": "#d5f2ff",
    "list.activeSelectionBackground": "#0096873a",
    "list.errorForeground": "#FF5252",
    "list.warningForeground": "#FFC135",
    //tab
    "tab.activeBorder": "#009688",
    "tab.activeForeground": "#DDDDDD",
    "tab.hoverBackground": "#282a2e",
    "tab.activeBackground": "#212121",
    "tab.inactiveBackground": "#191919",
    "editorGroupHeader.tabsBackground": "#191919",
    "tab.border": "#111111",
    // Peek View
    "peekViewEditor.background": "#191919",
    "peekView.border": "#009688",
    "peekViewEditor.matchHighlightBackground": "#ffffff3f",
    // Gutter
    "editorGutter.background": "#202020",
    // Breadcrumbs
    "breadcrumb.foreground": "#DDDDDD",
    "breadcrumb.background": "#161616",
    //Terminal
    "panel.background": "#191919",
    "panel.border": "#111111",
    "terminal.ansiWhite": "#d5f2ff",
    "terminal.ansiBlack": "#252525",
    "terminal.ansiBlue": "#42A5F5",
    "terminal.ansiCyan": "#00ACC1",
    "terminal.ansiGreen": "#C3D82C",
    "terminal.ansiMagenta": "#D81B60",
    "terminal.ansiRed": "#FF5252",
    "terminal.ansiYellow": "#FFC135",
    "terminal.ansiBrightWhite": "#d5f2ff",
    "terminal.ansiBrightBlack": "#252525",
    "terminal.ansiBrightBlue": "#42A5F5",
    "terminal.ansiBrightCyan": "#00ACC1",
    "terminal.ansiBrightGreen": "#C3D82C",
    "terminal.ansiBrightMagenta": "#D81B60",
    "terminal.ansiBrightRed": "#FF5252",
    "terminal.ansiBrightYellow": "#FFC135",
    //git
    "gitDecoration.deletedResourceForeground": "#FF5252",
    "gitDecoration.conflictingResourceForeground": "#89DDFF",
    "gitDecoration.modifiedResourceForeground": "#FFCB6B",
    "gitDecoration.untrackedResourceForeground": "#C3E88D",
    "gitDecoration.ignoredResourceForeground": "#84848490",
    // Diff Editor.
    // https: //code.visualstudio.com/docs/getstarted/theme-color-reference#_diff-editor-colors
    "diffEditor.insertedTextBackground": "#00ff000e",
    "diffEditor.insertedTextBorder": "#00ff009a",
    "diffEditor.removedTextBackground": "#ff000d1a",
    "diffEditor.removedTextBorder": "#ff000d81"
  },
  "tokenColors": [
    // General
    {
      "settings": {
        "foreground": "#d5f2ff"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4A4A4A"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C792EA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Numeric",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#f3a1d1"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.arithmetic.js",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    // CSS
    {
      "name": "[CSS] Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#63e4d8"
      }
    },
    {
      "name": "[CSS] size unit numbers",
      "scope": [
        "keyword.other.unit.px.css",
        "keyword.other.unit.percentage.css",
        "keyword.other.unit.ms.css",
        "keyword.other.unit.em.css",
        "keyword.other.unit.rem.css",
        "keyword.other.unit.s.css",
        "keyword.other.unit.vh.css",
        "keyword.other.unit.vw.css",
        "keyword.other.unit.deg.css",
        "keyword.other.unit.fr.css"
      ],
      "settings": {
        "foreground": "#bc9eff"
      }
    },
    {
      "name": "[CSS] size units and numeric values",
      "scope": [
        "constant.numeric.css",
        "support.constant.property-value.css",
        "support.constant.font-name.css",
        "support.constant.color.w3c-standard-color-name.css"
      ],
      "settings": {
        "foreground": "#f3a1d1"
      }
    },
    {
      "name": "[CSS] Words",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.scss"
      ],
      "settings": {
        "foreground": "#d5f2ff"
      }
    },
    {
      "name": "[CSS] ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "[CSS] SASS",
      "scope": [
        "keyword.control.at-rule.include.scss",
        "support.constant.media.css",
        "entity.other.attribute-name.pseudo-class.css",
        "support.constant.mathematical-symbols.scss"
      ],
      "settings": {
        "foreground": "#C792EA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "[CSS] !important",
      "scope": [
        "keyword.other.important.css",
        "keyword.other.important.scss",
        "keyword.control.case.php",
        "keyword.control.default.php"
      ],
      "settings": {
        "foreground": "#ff748b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "[CSS] Color Names",
      "scope": ["support.constant.color.w3c-extended-color-name.css"],
      "settings": {
        "foreground": "#ff748b"
      }
    },
    // PHP
    {
      "name": "[PHP] syntax echo",
      "scope": [
        "support.function.construct.output.php",
        "keyword.control.return.php",
        "keyword.control.flow.js"
      ],
      "settings": {
        "foreground": "#6dc7ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "[JS] object key",
      "scope": ["constant.other.object.key.js"],
      "settings": {
        "foreground": "#f3a1d1"
      }
    },
    {
      "name": "[PHP] syntax loops",
      "scope": ["keyword.control", "meta.for.js keyword.control.loop.js"],
      "settings": {
        "foreground": "#C792EA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#d5f2ff"
      }
    },
    {
      "name": "Variables in SCSS",
      "scope": ["variable", "variable.scss"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#d5f2ff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#f07178"
        // "foreground": "#F78C6C"
      }
    },
    {
      "name": "String Quoted",
      "scope": ["support.type.object.dom", "support.type.object.console.js"],
      "settings": {
        "foreground": "#FFCB6B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        //"fontStyle": "normal",
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        //"support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#ffe082"
        //"fontStyle": "italic"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#d5f2ff"
        //"fontStyle": "italic",
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx"
